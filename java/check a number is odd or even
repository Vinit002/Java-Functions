package com.company;

import java.util.*;

public class Main {

    public static void Check(int n) {

            if (n%2 == 0) {
                System.out.println("Even");
                return;
            }

        System.out.println("Odd");

    }


    public static void main (String[] args) {

            Scanner sc = new Scanner(System.in);
                int n = sc.nextInt();

                Check(n);

        }
    }
